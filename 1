#include <iostream>
#include <string>
using namespace std;

int main() 
{
 int a = -5;          // Integer variable
 cout<<a<<endl;

 float b = 4.0;         // Float variable
 cout<<b<<endl;

 string s = "abcd" ;  // String variable
 cout<<s<<endl;

 int ar[4]={7,8,9,10};   // Integer array
cout<<ar[2];

  return 0;
}
